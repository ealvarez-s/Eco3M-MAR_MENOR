#
#
#

#  Rule from ".f" to ".o"
#
.F90.o:
#	$(CPP)  $(KEY1)  $< $(SED) > $(OBJDIR)/$*.f90
#	cd $(OBJDIR); $(F90) $(OPTIONM) -c   $(OBJDIR)/$*.f90
	echo $(VPATH); 
	$(CPP)  $(KEY1) $(KEY2)  $< $(SED) > $(OBJDIR)/$*.f90
	$(F90) $(OPTIONM) -c   $(OBJDIR)/$*.f90
	@mv  $*.o $(OBJDIR)/.

